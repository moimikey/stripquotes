{
  "name": "stripquotes",
  "version": "0.2.4",
  "description": "strip leading/trailing single or double quotes from a string",
  "main": "lib/index.js",
  "scripts": {
    "pile": "babel -d lib/ src/",
    "build": "npm run pile",
    "test": "babel-node test.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moimikey/stripquotes.git"
  },
  "keywords": [
    "strip quotes",
    "strip quote",
    "stripquotes",
    "stripquote",
    "strip",
    "quote",
    "replace"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com> (http://hertzber.gs/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moimikey/stripquotes/issues"
  },
  "homepage": "https://github.com/moimikey/stripquotes#readme",
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-core": "6.4.5",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3",
    "babel-preset-stage-0": "6.5.0",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
